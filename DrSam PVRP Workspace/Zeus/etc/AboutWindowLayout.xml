<?xml version="1.0" ?>
<!DOCTYPE constraint-set
    PUBLIC "-//Brunch Boy Design//RelativeLayout Constraint Set DTD 1.0//EN"
    "http://dtd.brunchboy.com/RelativeLayout/constraint-set.dtd">

<constraint-set>
        <constrain name="label_1">
          <top>
            <toAttribute reference="_container" attribute="top" offset="5" />
          </top>
          <left>
            <toAttribute reference="_container" attribute="left" offset="3" />
          </left>
          <right>
            <toAttribute reference="_container" attribute="right" offset="-3" />
          </right>
        </constrain>
        <constrain name="label_2">
          <top>
            <toAttribute reference="label_1" attribute="bottom" offset="5" />
          </top>
          <left>
            <toAttribute reference="_container" attribute="left" offset="3" />
          </left>
          <right>
            <toAttribute reference="_container" attribute="right" offset="-3" />
          </right>
        </constrain>
        <constrain name="label_3">
          <top>
            <toAttribute reference="label_2" attribute="bottom" offset="5" />
          </top>
          <left>
            <toAttribute reference="_container" attribute="left" offset="3" />
          </left>
          <right>
            <toAttribute reference="_container" attribute="right" offset="-3" />
          </right>
        </constrain>
        <constrain name="label_4">
          <top>
            <toAttribute reference="label_3" attribute="bottom" offset="5" />
          </top>
          <left>
            <toAttribute reference="_container" attribute="left" offset="3" />
          </left>
          <right>
            <toAttribute reference="_container" attribute="right" offset="-3" />
          </right>
        </constrain>
        <constrain name="label_5">
          <top>
            <toAttribute reference="label_4" attribute="bottom" offset="5" />
          </top>
          <left>
            <toAttribute reference="_container" attribute="left" offset="3" />
          </left>
          <right>
            <toAttribute reference="_container" attribute="right" offset="-3" />
          </right>
        </constrain>
        <constrain name="label_6">
          <top>
            <toAttribute reference="label_5" attribute="bottom" offset="5" />
          </top>
          <left>
            <toAttribute reference="_container" attribute="left" offset="3" />
          </left>
          <right>
            <toAttribute reference="_container" attribute="right" offset="-3" />
          </right>
        </constrain>
        <constrain name="closeButton">
          <bottom>
            <toAttribute reference="_container" attribute="bottom" offset="-5" />
          </bottom>
          <horizontalCenter>
            <toAttribute reference="_container" attribute="horizontalCenter"/>
          </horizontalCenter>
        </constrain>
</constraint-set>

